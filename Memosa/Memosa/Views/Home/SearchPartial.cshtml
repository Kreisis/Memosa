@model Dto.ThumbnailList

@{
    Layout = null;
}

@for (int x = 0; x < Model.thumbnails.Count; x++)
{
    Dto.Thumbnail tb = Model.thumbnails[x];
    <div class="col-sm-6 col-md-6 col-lg-4">
        <a href="/Content/Meme/@tb.ID">
            <div class="thumbnail-wrapper">
                <div class="thumbnail-holder">
                    <div class="img-wrapper">
                        <img class="thumbnail-img" src="@tb.ImagePath">
                        <h1 class="thumbnail-text">@tb.Title</h1>
                    </div>
                </div>
            </div>
        </a>

    </div>

}
